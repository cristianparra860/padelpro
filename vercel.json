{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "crons": [
    {
      "path": "/api/cron/generate-cards?daysRange=30",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/admin/generate-proposals",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/generate-matches",
      "schedule": "0 2 * * *"
    }
  ]
}